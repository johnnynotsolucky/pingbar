{
  "name": "tutorial-menubar-app",
  "author": "Tyrone Tudehope",
  "version": "1.0.0",
  "description": "A simple Electron tutorial app for the menubar",
  "license": "MIT",
  "repository": "github.com/ryanbaer/simple-electron-app",
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^19.37.2",
    "electron-packager": "^8.0.0",
    "electron-prebuilt": "^1.4.1"
  },
  "main": "main.js",
  "scripts": {
    "start-electron": "electron .",
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "build-osx": "rm -rf Ping-darwin-x64 && electron-packager . Ping --platform=darwin --arch=x64 --icon=Icon.icns",
    "dist": "electron-builder",
    "test-react": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier --write **/*.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "electron-config": "^1.0.0",
    "electron-positioner": "^3.0.0",
    "electron-store": "^1.3.0",
    "extend": "^3.0.1",
    "menubar": "^5.2.3",
    "ping": "^0.2.2",
    "prettier": "^1.7.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "^1.0.14",
    "styled-components": "^2.2.1",
    "typeface-open-sans": "^0.0.35"
  }
}
